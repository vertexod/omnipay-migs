{
    "name": "omnipay/migs",
    "type": "library",
    "description": "MIGS driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "mastercard internet gateway service",
        "merchant",
        "migs",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-migs",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-migs/contributors"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Migs\\" : "src/" }
    },
    "require": {
        "omnipay/common": "dev-client-common"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^0.14"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "repositories": [
	{
            "type": "vcs",
            "url": "https://github.com/vertexod/omnipay-migs.git"
        }
    ],
    "prefer-stable": true
}
